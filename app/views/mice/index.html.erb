<!-- Services -->
<section id="services">
    <div class="container">
        <div class="row">
            <div class="col-lg-12 text-center">
            <h2 class="section-heading text-uppercase">Mice</h2>
            <h3 class="section-subheading text-muted">Your Computing Mice Needs.</h3>
            <div class="row text-center">
        <div class="col-md-4">
        <span class="fa-stack fa-4x">
            <i class="fas fa-circle fa-stack-2x text-primary"></i>
            <i class="fas fa-laptop fa-stack-1x fa-inverse"></i>
        </span>
        <p class="text-muted">
            <ul>
            <% @mice.each do |mouse| %>
                <li> 
                    <%= link_to mouse.name, mouse %> 
                </li>
            <% end %>
            </ul>
        </p>
        </div>
            </div>
        </div>
        </div>
    </div>
</section>
